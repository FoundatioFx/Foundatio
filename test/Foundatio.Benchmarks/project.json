{
    "buildOptions": {
        "debugType": "portable",
        "emitEntryPoint": true,
        "optimize": true,
        "warningsAsErrors": true
    },
    "dependencies": {
        "BenchmarkDotNet": "0.9.8",
        "Foundatio": {
            "target": "project"
        },
        "Foundatio.Redis": {
            "target": "project"
        }
    },
    "frameworks": {
        "netcoreapp1.0": {
            "buildOptions": {
                "define": [ "NETSTANDARD", "NETSTANDARD1_5" ]
            },
            "dependencies": {
                "Microsoft.NETCore.App": {
                    "type": "platform",
                    "version": "1.0.0"
                }
            }
        },
      "NET46": {
        "buildOptions": {
          "define": [ "NET46" ]
        },
        "frameworkAssemblies": {
          "System.Threading.Tasks": {
            "type": "build"
          },
          "System.Runtime": {
            "type": "build"
          }
        }
      }
    }
}