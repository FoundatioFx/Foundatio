{
  "title": "RabbitMQ Publisher Console",
    "buildOptions": {
        "debugType": "portable",
        "emitEntryPoint": true,
        "optimize": true,
        "warningsAsErrors": true
    },
  "dependencies": {

    "Foundatio": {
      "target": "project"
    },
    "Foundatio.Logging.NLog": {
      "target": "project"
    },
    "Foundatio.RabbitMQ": {
      "target": "project"
    }

  },
    "frameworks": {
        "NET46": {
            "buildOptions": {
                "define": [ "NET46" ]
            },
            "frameworkAssemblies": {
                "System.Threading.Tasks": {
                    "type": "build"
                },
                "System.Runtime": {
                    "type": "build"
                }
            }
        }
    }
}